{
  "version": 2,
  "builds": [
    {
      "src": "apps/platform/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/platform/$1"
    }
  ],
  "env": {
    "SITE_ID": "vpn-service-01"
  }
}
